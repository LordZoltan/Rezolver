<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <Copyright>2014 onwards Zolution Software Ltd</Copyright>
    <Authors>Andras Zoltan</Authors>
    <Company>Zolution Software Ltd</Company>
    <PackageReleaseNotes>See https://github.com/ZolutionSoftware/Rezolver/releases</PackageReleaseNotes>
    <PackageIcon>icon64x64.png</PackageIcon>
    <PackageProjectUrl>http://rezolver.co.uk</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Product>Rezolver</Product>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/ZolutionSoftware/Rezolver</RepositoryUrl>
    <PreReleaseSuffix>$([System.DateTime]::Now.ToString('yyMMddHHmm'))</PreReleaseSuffix>
    <PreReleaseType Condition ="'$(PreReleaseType)' == ''">alpha</PreReleaseType>
    <VersionSuffix Condition="'$(VersionSuffix)' == '' AND '$(Configuration)' != 'Release'">$(PreReleaseType)-$(PreReleaseSuffix)</VersionSuffix>
  </PropertyGroup>
</Project>
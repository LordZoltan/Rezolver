{
  "version": "1.0.0-alpha-200",
  "description": "Logging classes for Rezolver",
  "authors": [ "Zolution Software Ltd" ],
  "packOptions": {
    "tags": [ "ioc", "di", "portable", "rezolver", "logging" ],
    "projectUrl": "http://rezolver.co.uk",
    "licenseUrl": ""
  },
  "dependencies": {
    "Rezolver": "1.0.0-alpha-*",
    "Rezolver.Shared": {
      "version": "1.0.10-alpha-*",
      "type": "build"
    }
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": { "System.Linq.Expressions": "4.0.0.0" }
    },
    "net46": {
      "frameworkAssemblies": { "System.Linq.Expressions": "4.0.10.0" }
    },
    "netstandard1.1": {
      "buildOptions": { "define": [ "DOTNET" ] },
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    }
  },
  "scripts": { "postcompile": [ "dotnet pack --output ../../.nupkg --no-build --configuration %compile:Configuration% " ] }

  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "Microsoft.CSharp": "4.0.0.0",
        "System": "4.0.0.0",
        "System.Core": "4.0.0.0",
        "System.Linq": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Xml.Linq": "4.0.0.0"
      }
    },
    "netcore45": {
      "compilationOptions": { "define": [ "DOTNET" ] },
      "frameworkAssemblies": {
        "Microsoft.CSharp": "4.0.0.0",
        "System": "4.0.0.0",
        "System.Collections": "4.0.0.0",
        "System.Collections.Concurrent": "4.0.0.0",
        "System.ComponentModel": "4.0.0.0",
        "System.Diagnostics.Debug": "4.0.0.0",
        "System.Diagnostics.Tools": "4.0.0.0",
        "System.Dynamic.Runtime": "4.0.0.0",
        "System.Core": "4.0.0.0",
        "System.Linq": "4.0.0.0",
        "System.Linq.Expressions": "4.0.0.0",
        "System.Reflection": "4.0.0.0",
        "System.Reflection.Extensions": "4.0.0.0",
        "System.Reflection.Primitives": "4.0.0.0",
        "System.Resources.ResourceManager": "4.0.0.0",
        "System.Runtime": "4.0.0.0",
        "System.Text.RegularExpressions": "4.0.0.0",
        "System.Threading": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Xml.Linq": "4.0.0.0"
      }
    },
    "wpa81": {
      "compilationOptions": { "define": [ "DOTNET" ]},
      "frameworkAssemblies": {
        "Microsoft.CSharp": "4.0.0.0",
        "System": "4.0.0.0",
        "System.Collections": "4.0.0.0",
        "System.Collections.Concurrent": "4.0.0.0",
        "System.ComponentModel": "4.0.0.0",
        "System.Diagnostics.Debug": "4.0.0.0",
        "System.Diagnostics.Tools": "4.0.0.0",
        "System.Dynamic.Runtime": "4.0.0.0",
        "System.Core": "4.0.0.0",
        "System.Linq": "4.0.0.0",
        "System.Linq.Expressions": "4.0.0.0",
        "System.Reflection": "4.0.0.0",
        "System.Reflection.Extensions": "4.0.0.0",
        "System.Reflection.Primitives": "4.0.0.0",
        "System.Resources.ResourceManager": "4.0.0.0",
        "System.Runtime": "4.0.10.0",
        "System.Text.RegularExpressions": "4.0.0.0",
        "System.Threading": "4.0.0.0",
        "System.Xml": "4.0.0.0",
        "System.Xml.Linq": "4.0.0.0"
      }
    },
    "dotnet": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.0-beta-*",
        "System.Collections": "4.0.10-beta-*",
        "System.Collections.Concurrent": "4.0.10-beta-*",

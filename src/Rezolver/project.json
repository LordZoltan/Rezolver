{
  "description": "Rezolver for CoreCLR - supports vNext Core and Full frameworks, Net45 and Net46",
  "version": "0.5.2-alpha-902",
  "tags": [ "ioc", "di", "portable", "rezolver" ],
  "authors": [ "Andras Zoltan" ],
  "dependencies": {
    "Rezolver.Shared": {
      "version": "1.0.1-*",
      "type": "build"

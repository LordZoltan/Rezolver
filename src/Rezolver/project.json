{
  
  "version": "1.1.71702.100",
  "title": "Rezolver IOC Framework",
  "description": "Rezolver is a fast IOC framework offering all the most common features of any IOC container.  It's easy to use and 100% extensible.",
  "packOptions": {
    "summary": "Rezolver - An IOC/DI framework",
    "tags": [ "ioc", "di", "portable", "rezolver" ],
    "projectUrl": "http://rezolver.co.uk",
    "iconUrl": "http://rezolver.co.uk/favicon-96x96.png"
  },
  "authors": [ "Zolution Software Ltd" ],
  "copyright": "2014 onwards Zolution Software Ltd",
  "dependencies": {
    "Rezolver.Shared": {
      "version": "1.1.0-*",
      "type": "build"
    }

  },
  "frameworks": {
    "netstandard1.1": {
      "buildOptions": { "define": [ "DOTNET" ] },
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.ComponentModel": "4.0.1"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.ComponentModel": "4.0.0.0"
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.ComponentModel": "4.0.0.0"
      }
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "define": [ "Wra" ]
  },
  "scripts": { "postcompile": [ "dotnet pack --output ../../.nupkg --no-build --configuration %compile:Configuration% " ] }
}

{
  "description": "Rezolver - A new dependency injection framework",
  "version": "1.0.0-alpha-2",
  "packOptions": {
    "tags": [ "ioc", "di", "portable", "rezolver" ],
    "projectUrl": "http://www.zolution.co.uk/Rezolver"
  },
  "authors": [ "Andras Zoltan" ],
  "dependencies": {
    "Rezolver.Shared": {
      "version": "1.0.10-*",
      "type": "build"
    }
  },
  "frameworks": {
    "netstandard1.5": {
      "buildOptions": { "define": [ "DOTNET" ] },
      "dependencies": {
        "System.ComponentModel": "4.0.1-rc2-24027",
        "System.Resources.ResourceManager": "4.0.1-rc2-24027",
        "System.Diagnostics.Debug": "4.0.11-rc2-24027",
        "System.Threading": "4.0.11-rc2-24027"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.ComponentModel": "4.0.0.0"
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.ComponentModel": "4.0.0.0"
      }
    }
  },
  "buildOptions": {
    "xmlDoc": true
  }
}

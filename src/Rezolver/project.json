{
    "description": "Rezolver for CoreCLR - supports vNext Core and Full frameworks, Net45 and Net46",
    "version": "0.5.2-alpha-902",
    "tags": [ "ioc", "di", "portable", "rezolver" ],
    "authors": [ "Andras Zoltan" ],
    "dependencies": {
        "Rezolver.Shared": { "version": "1.0.1-*", "type": "build" }
    },
    "compile": [ "**/*.cs", "../Rezolver.Resources/CoreClrResources.cs" ],

    "frameworks": {
        "net45": {
            "frameworkAssemblies": {
                "System": "4.0.0.0",
                "System.Core": "4.0.0.0",
                "System.Xml.Linq": "4.0.0.0",
                "System.Xml": "4.0.0.0",
                "System.Linq": "4.0.0.0",
                "Microsoft.CSharp": "4.0.0.0"
            }
        },
        "dnx451": {
            "frameworkAssemblies": {
                "System": "4.0.0.0",
                "System.Core": "4.0.0.0",
                "System.Xml.Linq": "4.0.0.0",
                "System.Xml": "4.0.0.0",
                "System.Linq": "4.0.0.0",
                "Microsoft.CSharp": "4.0.0.0"
            }
        },
        "dotnet": {
            "dependencies": {
                "System.ComponentModel": "4.0.0-beta-*",
                "System.Diagnostics.Debug": "4.0.10-beta-*",
                "System.Diagnostics.Tools": "4.0.0-beta-*",
                "System.Runtime": "4.0.20-beta-*",
                "System.Reflection": "4.0.10-beta-*",
                "System.Reflection.Extensions": "4.0.0-beta-*",
                "System.Reflection.Primitives": "4.0.0-beta-*",
                "System.Reflection.TypeExtensions": "4.0.0-beta-*",
                "System.Linq": "4.0.0-beta-*",
                "System.Linq.Expressions": "4.0.10-beta-*",
                "System.Collections": "4.0.10-beta-*",
                "System.Collections.Concurrent": "4.0.10-beta-*",
                "System.Resources.ResourceManager": "4.0.0-beta-*",
                "System.Text.RegularExpressions": "4.0.10-beta-*",
                "System.Threading": "4.0.10-beta-*",
                "Microsoft.CSharp": "4.0.0-beta-*"
            }
        }
    }
}

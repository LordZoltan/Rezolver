{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "rezolver.csproj"
          ],
          "cwd": "../../src/Rezolver"
        },
        {
          "files": [
            "rezolver.microsoft.aspnetcore.hosting.csproj"
          ],
          "cwd": "../../src/Rezolver.Microsoft.AspNetCore.Hosting"
        },
        {
          "files": [
            "rezolver.microsoft.extensions.dependencyinjection.csproj"
          ],
          "cwd": "../../src/Rezolver.Microsoft.Extensions.DependencyInjection"
        }
      ],
      "dest": "_docfx_proj/obj/api",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "Rezolver IOC",
      "_description": "Rezolver is the portable open-source IOC container framework for .Net - built from the ground up for .Net Core, Asp.Net Core, and also optimised for the 'full .Net' :)",
      "_appFooter": "<span>Copyright &copy;2014 onwards <a href=\"http://www.zolution.co.uk\" target=\"_blank\">Zolution Software Ltd</a><br>Generated by <a href=\"https://dotnet.github.io/docfx/\" target=\"_blank\">DocFX</a>, with the Rezolver theme</span>",
      "_enableSearch": true,
      "_appLogoPath": "../content/rz_square_white_on_orange_48x48.png",
      "_packageId": "Rezolver",
      "_packagePreRelease": false,
      "_githubPath": "ZolutionSoftware/Rezolver",
      "_usingWaffle": true,
      "_GATracking": false,
      "_GATrackingID": "UA-92764222-1"
    },
    "xrefservice": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "src": "_docfx_proj/obj"
      },
      {
        "files": [
          "api/*.md",
          "docs/**.md",
          "docs/**.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**"
        ],
        "src": "_docfx_proj"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**"
        ],
        "src": "_docfx_proj"
      }
    ],
    "overwrite": [
      {
        "files": [
          "api-overwrite/**.md"
        ],
        "exclude": [
          "obj/**"
        ],
        "src": "_docfx_proj"
      }
    ],
    "dest": "wwwroot/developers",
    "template": [
      "_docfx_themes/default",
      "_docfx_themes/rezolver"
    ]
  }
}
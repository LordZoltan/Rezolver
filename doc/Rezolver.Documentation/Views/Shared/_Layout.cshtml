<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title | Rezolver IOC Framework</title>
    <link rel="stylesheet" href="@Url.Content("~/developers/styles/docfx.vendor.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/developers/styles/docfx.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/developers/styles/main.css")" type="text/css" />
    <meta property="docfx:navrel" content="@Url.Content("~/developers/toc.html")" />
    <meta property="docfx:tocrel" content="@Url.Content("~/developers/toc.html")" />
    <meta property="docfx:rel" content="@Url.Content("~/developers/")" />
    <link rel="shortcut icon" href="@Url.Content("~/favicon.ico")">
    <link rel="apple-touch-icon" sizes="57x57" href="@Url.Content("~/apple-touch-icon-57x57.png")">
    <link rel="apple-touch-icon" sizes="60x60" href="@Url.Content("~/apple-touch-icon-60x60.png")">
    <link rel="apple-touch-icon" sizes="72x72" href="@Url.Content("~/apple-touch-icon-72x72.png")">
    <link rel="apple-touch-icon" sizes="76x76" href="@Url.Content("~/apple-touch-icon-76x76.png")">
    <link rel="apple-touch-icon" sizes="114x114" href="@Url.Content("~/apple-touch-icon-114x114.png")">
    <link rel="apple-touch-icon" sizes="120x120" href="@Url.Content("~/apple-touch-icon-120x120.png")">
    <link rel="apple-touch-icon" sizes="144x144" href="@Url.Content("~/apple-touch-icon-144x144.png")">
    <link rel="apple-touch-icon" sizes="152x152" href="@Url.Content("~/apple-touch-icon-152x152.png")">
    <link rel="apple-touch-icon" sizes="180x180" href="@Url.Content("~/apple-touch-icon-180x180.png")">
    <link rel="icon" type="image/png" href="@Url.Content("~/favicon-32x32.png")" sizes="32x32">
    <link rel="icon" type="image/png" href="@Url.Content("~/android-chrome-192x192.png")" sizes="192x192">
    <link rel="icon" type="image/png" href="@Url.Content("~/favicon-96x96.png")" sizes="96x96">
    <link rel="icon" type="image/png" href="@Url.Content("~/favicon-16x16.png")" sizes="16x16">
    <link rel="mask-icon" href="@Url.Content("~/safari-pinned-tab.svg")" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-TileImage" content="@Url.Content("~/mstile-144x144.png")">
    <meta name="theme-color" content="#ffffff">
</head>
<body>
    <div id="wrapper">
        <header>
            <nav id="autocollapse" class="navbar navbar-inverse ng-scope" role="navigation">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>

                        <a class="navbar-brand" href="@Url.Content("~/")">
                            <img id="logo" class="svg" src="@Url.Content("~/content/rz_square_white_on_orange_48x48.png")" alt="Rezolver Logo">
                        </a>
                    </div>
                    <div class="collapse navbar-collapse" id="navbar">
                        <p class="navbar-text navbar-right github-link"><a href="http://github.com/zolutionsoftware/rezolver" title="See this project on Github"><span class="fa fa-github"></span></a></p>
                        <form class="navbar-form navbar-right" role="search" id="search">
                            <div class="form-group">
                                <input type="text" class="form-control" id="search-query" placeholder="Search" autocomplete="off">
                            </div>
                        </form>
                        @*<ul class="nav level1 navbar-nav">
                            <li>
                                <a href="@Url.Content("~/")">Home</a>
                            </li>
                        </ul>*@
                    </div>
                </div>
            </nav>
            <div class="subnav navbar navbar-default">
                <div id="breadcrumb" class="container hide-when-search">
                    <ul class="breadcrumb">
                        <li>@ViewBag.Title</li>
                    </ul>
                </div>
            </div>
        </header>
        <div id="search-results">
            <div class="search-list"></div>
            <div class="sr-items"></div>
            <ul id="pagination"></ul>
        </div>

        <div role="main" class="container body-content hide-when-search">
            <div class="article row grid">
                <div class="col-md-10">
                    <article id="_content" class="content wrap">
                        @RenderBody()
                    </article>
                </div>
            </div>


        </div>
        <footer>
            <div class="footer">
                <div class="container">
                    <span class="pull-right">
                        <a href="#top">Back to top</a>
                    </span>
                    <span>Copyright &copy;2014 onwards <a href="http://www.zolution.co.uk" target="_blank">Zolution Software Ltd</a><br>Generated by <a href="https://dotnet.github.io/docfx/" target="_blank">DocFX</a>, with the Rezolver theme</span>

                </div>
            </div>
        </footer>
    </div>
    @* contains jquery and bootstrap *@
    <script type="text/javascript" src="@Url.Content("~/developers/styles/docfx.vendor.js")"></script>
    @* docfx scripts*@
    <script type="text/javascript" src="@Url.Content("~/developers/styles/docfx.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/developers/styles/main.js")"></script>
</body>
</html>

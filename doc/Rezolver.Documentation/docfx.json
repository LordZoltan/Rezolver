{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.cs"
          ],
          "cwd": "../../src/Rezolver",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        },
        {
          "files": [
            "**/*.cs"
          ],
          "cwd": "../../src/Rezolver.Configuration",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        },
        {
          "files": [
            "**/*.cs"
          ],
          "cwd": "../../src/Rezolver.Configuration.Json",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        },
        {
          "files": [
            "**/*.cs"
          ],
          "cwd": "../../src/Rezolver.Microsoft.Extensions.DependencyInjection",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appFooter": "<span>Copyright &copy;2015-2016 Zolution Software Ltd<br>Generated by <a href=\"https: //dotnet.github.io/docfx/\" target=\"_blank\">DocFX</a>, with the Rezolver theme</span>",
      "_enableSearch": true
    },
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "articles/**.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "learn-rezolver/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "learn-rezolver/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "learn-rezolver/**"
        ]
      }
    ],
    "dest": "learn-rezolver",
    "template": [
      "default",
      "../Rezolver.Theme"
    ]
  }
}
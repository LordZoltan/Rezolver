{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "project.json"
          ],
          "cwd": "../../src/Rezolver",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appFooter": "<span>Copyright &copy;2015 onwards <a href=\"http://www.zolution.co.uk\" target=\"_blank\">Zolution Software Ltd</a><br>Generated by <a href=\"https: //dotnet.github.io/docfx/\" target=\"_blank\">DocFX</a>, with the Rezolver theme</span>",
      "_enableSearch": true,
      "_appLogoPath": "../content/logo.png"
    },
    "xref": [
      "msdn.4.5.2.zip",
      "namespaces.4.5.2.zip"
    ],
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "rezolver-api/*.md",
          "rezolver-usage/**.md",
          "rezolver-usage/**.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "learn/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "learn/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "learn/**"
        ]
      }
    ],
    "dest": "learn",
    "template": [
      "default"
    ]
  }
}
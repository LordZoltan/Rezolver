{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "project.json"
          ],
          "cwd": "../../src/Rezolver",
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        }
      ],
      "dest": "_docfx_proj/obj/api"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appFooter": "<span>Copyright &copy;2015 onwards <a href=\"http://www.zolution.co.uk\" target=\"_blank\">Zolution Software Ltd</a><br>Generated by <a href=\"https://dotnet.github.io/docfx/\" target=\"_blank\">DocFX</a>, with the Rezolver theme</span>",
      "_enableSearch": true,
      "_appLogoPath": "../content/rz_square_white_on_orange_48x48.png",
      "_packageVersion": "1.1.71702.100",
      "_packageReleased": true,
      "_githubUrl":  "http://github.com/zolutionsoftware/rezolver"
    },
    "xref": [
      "msdn.4.5.2.zip",
      "namespaces.4.5.2.zip"
    ],
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "_docfx_proj/obj"
      },
      {
        "files": [
          "api/*.md",
          "docs/**.md",
          "docs/**.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**"
        ],
        "cwd": "_docfx_proj"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**"
        ],
        "cwd": "_docfx_proj"
      }
    ],
    "overwrite": [
      {
        "files": [
          "api-overwrite/**.md"
        ],
        "exclude": [
          "obj/**"
        ],
        "cwd": "_docfx_proj"
      }
    ],
    "dest": "developers",
    "template": [
      "_docfx_themes/default",
      "_docfx_themes/rezolver"
    ]
  }
}